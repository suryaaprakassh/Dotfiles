font_family Jetbrains Mono Nerd Font

italic_font auto

bold_font auto

bold_italic_font auto

font_size 14.0

cursor_shape block
enable_audio_bell no
shell_integration no-title

background_opacity 0.3

draw_minimal_borders yes
url_style curly
shell zsh 

# BEGIN_KITTY_THEME
# Solarized Light
include current-theme.conf
# END_KITTY_THEME
